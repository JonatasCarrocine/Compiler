/* Um programa para calcular o 
mdc segundo o algoritimo de Euclides. */

int gcd (int u, int v)
{
    if (v == 0) return u ;
    else return gcd(v,u-u/v*v);
    /* u-u/v*v == u mod v */
}

void main(void)
{
    int x;
    int y;
    x = 4;
    y = 2;
    /*x = input();    y = input();*/
    output(gcd(x,y));
}